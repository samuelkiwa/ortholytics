#!/usr/local/bin/perl

use Bio::SeqIO;

$infile = shift;

$in  = Bio::SeqIO->new(-file => "$infile" , 
                       -format => 'largefasta');
my %chrs;
while ( my $seq = $in->next_seq() ) {
    my $id = $seq->id;
    $chrs{$id} = $seq;
#    print "Sequence ",$seq->id, " first 10 bases ", $seq->subseq(1,10), "\n";
   print "Sequence ",$seq->id, " length ", $seq->length, "\n";
}





